# 生產環境變量，解決 GitHub Pages 上的動畫播放問題
VITE_APP_ENV=production

# GitHub Pages 相關設置
VITE_PUBLIC_URL=./

# 動畫相關設置
VITE_ENABLE_ANIMATIONS=true
VITE_ANIMATION_DELAY=100

# 調試模式
VITE_DEBUG_MODE=false

# 資源路徑前綴
VITE_ASSET_PREFIX=./

# 設置 API 路徑 (如果有的話)
# VITE_API_URL=https://api.example.com

# Motion 庫配置
VITE_USE_MOTION_FALLBACK=true